{
    "name": "parables/laravel-model-nanoid",
    "description": "This package allows you to easily work with NanoID in your Laravel models.",
    "license": "MIT",
    "keywords": [
        "eloquent",
        "laravel",
        "model",
        "nanoId"
    ],
    "authors": [
        {
            "name": "Parables Boltnoel",
            "email": "parables95@gmail.com",
            "homepage": "https://github.com/parables"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "snortlin/nano-id": "^1.0.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Parables\\NanoId\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
